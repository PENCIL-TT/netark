{
  "version": 2,
  "name": "netark",
  "public": true,
  "framework": "vite",
  "installCommand": "npm install",
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "env": {
    "VITE_APP_NAME": "Netark",
    "VITE_BRAND_COLOR": "#A1162A"
  }
}
