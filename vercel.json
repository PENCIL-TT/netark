{
  "version": 2,
  "name": "netark",
  "public": true,
  "installCommand": "npm install",
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_APP_NAME": "Netark",
    "VITE_BRAND_COLOR": "#A1162A"
  }
}
